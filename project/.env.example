# Firebase Configuration
# Copy this file to .env and fill in your Firebase project details

VITE_FIREBASE_API_KEY=your-firebase-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# AI API Keys for smart chatbot
VITE_GROQ_API_KEY=your-groq-api-key-here
VITE_GEMINI_API_KEY=your-gemini-api-key-here
VITE_GEMINI_API_KEY_2=your-gemini-api-key-2-here
VITE_PERPLEXITY_API_KEY=your-perplexity-api-key-here

# Other API Keys
VITE_GITHUB_TOKEN=your-github-token-here
VITE_NEWS_API=your-news-api-key-here
